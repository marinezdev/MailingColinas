<script>

    $(document).ready(function () {

        $('#IndexHome').addClass("nav-item active submenu");

        $.fn.dataTable.moment("DD/MM/YYYY");

        var formattedOutput = new Intl.NumberFormat('en-US', {
            style: 'currency',
