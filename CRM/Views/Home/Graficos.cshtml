        });

        $('#proyectos').DataTable({
            "autoWidth": false,
            "bDestroy": true,
            "colReorder": true,
            "pageLength": 20,
            "language": {
                'url': '//cdn.datatables.net/plug-ins/9dcbecd42ad/i18n/Spanish.json',
                'decimal': '.',
                'thousands': ','
            },
            "scrollX": true,
            columnDefs: [
                {
                    targets: [3],
                    className: "text-right",
                    render: $.fn.dataTable.render.number(',', '.', 2,'$')
                },{
                    targets: [9],
                    className: "text-center",
                }
            ],
            //'rowCallback': function (row, data, index) {
            //    if (data[3] > 200000) {
            //        $(row).find('td:eq(3)').css('color', 'lime');
            //    }
            //},
            "footerCallback": function (row, data, start, end, display) {
                var api = this.api(), data;

                // convirtiendo a entero para encontrar el total
                var intVal = function (i) {
                    return typeof i === 'string' ?
                        i.replace(/[\$,]/g, '') * 1 :
                        typeof i === 'number' ?
                            i : 0;
                };
                
                // sumando la columna Total
                var monTotal = api
                    .column(3)
                    .data()
                    .reduce(function (a, b) {
                        return intVal(a) + intVal(b);
                    }, 0);
                $(api.column(3).footer()).html(formattedOutput.format(monTotal).replace('₺', '$'));
            },
        });

        $('a.toggle-vis').on('click', function (e) {
            e.preventDefault();

            // Get the column API object
            var column = $('#basic-datatables-').DataTable().column($(this).attr('data-column'));

            // Toggle the visibility
            column.visible(!column.visible());
        });

